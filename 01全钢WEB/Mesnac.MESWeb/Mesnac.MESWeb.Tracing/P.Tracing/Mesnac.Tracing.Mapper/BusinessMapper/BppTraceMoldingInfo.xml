     <select id="GetCuringGroupInfo" parameterClass="map" resultClass="Row">
       <![CDATA[
       SELECT T.TYRE_NO BARCODE,T.TYRE_NO ID ,T.SHIFT_DATE,T1.MATERIAL_NAME MATERIALNAME,
      T2.SHIFT_NAME,T3.CLASS_NAME,T4.EQUIP_NAME,
      '日期：'||to_char(t.shift_date,'yyyy-MM-dd')||' 机台：'||t4.equip_name||' 班次：'||t2.shift_name||' 班组：'||t3.class_name G 
      FROM CPP_CURING_PRODUCTION T
      LEFT JOIN SBM_MATERIAL T1 ON T1.OBJID = T.MATERIAL_ID
      LEFT JOIN SSB_SHIFT T2 ON T2.OBJID = T.SHIFT_ID
      LEFT JOIN SSB_CLASS T3 ON T3.OBJID = T.CLASS_ID
      LEFT JOIN SBE_EQUIP T4 ON T4.OBJID = T.EQUIP_ID
      WHERE T.DELETE_FLAG=0
       ]]>
       <isNotNull property="Barcode" prepend="AND">
         <![CDATA[T.GREEN_TYRE_NO=#Barcode#]]>
       </isNotNull>
