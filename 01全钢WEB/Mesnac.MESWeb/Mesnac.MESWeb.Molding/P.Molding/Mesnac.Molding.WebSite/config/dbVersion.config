<?xml version="1.0" encoding="utf-8"?>
<configuration >
  <DbSource version="Default" remark="默认数据库">
    <DbInfo DbName="Default" MapperName="SqlMap.config" />
    <DbInfo DbName="SqlPlm" MapperName="SqlPlm.config" />
  </DbSource>
  <DbSource version="Local" remark="本地数据库">
    <DbInfo DbName="Default" MapperName="Local.SqlMap.config" />
  </DbSource>


  <DbMapper pluginName="Main" dbName="Default"/>
  <DbMapper pluginName="Molding" dbName="Default"/>
  <DbMapper pluginName="FrameDb" dbName="Default"/>
  <DbMapper pluginName="MoldingPlm" dbName="SqlPlm"/>
</configuration>