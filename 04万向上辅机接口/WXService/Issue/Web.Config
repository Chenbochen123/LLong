<?xml version="1.0"?>
<configuration>
	<system.webServer>
		<defaultDocument>
			<files>
				<add value="Service.asmx"/>
			</files>
		</defaultDocument>
	</system.webServer>
	<system.web>
		<compilation debug="true"/>
    <webServices>
      <protocols>
        <add name="HttpSoap"/>
        <add name="HttpPost"/>
        <add name="HttpGet"/>
        <add name="Documentation"/>
      </protocols>
    </webServices>
  </system.web>
</configuration>
